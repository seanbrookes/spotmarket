{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 4545,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false
}
